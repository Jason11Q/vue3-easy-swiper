{
  "name": "vue3-easy-swiper",
  "author": {
    "name": "yezipi"
  },
  "files": [
    "dist", "lib"
  ],
  "description": "a easy vue3 loop swiper plugin",
  "version": "1.0.7",
  "module": "./dist/index.es.ts",
  "main": "./dist/index.umd.ts",
  "typings": "./lib/typings/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.ts",
