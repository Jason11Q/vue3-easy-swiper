      "require": "./dist/style.css"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yezipi/vue3-easy-swiper"
  },
  "keywords": ["swiper", "vue3"],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue-tsc": "^0.29.8"
  }
}
